defn fib (n) :
   if n == 0 :
      1
   else if n == 1 :
      1
   else :
      var a = 1
      var b = 1
      while n >= 2 :
         var c = a + b
         a = b
         b = c
         n = n - 1
      b   

defn main () :
   var i = 0
   while i < 20 :
      printf("Fib(~) = ~\n", i, fib(i))
      i = i + 1

main()



